{
   "description": "Install APT packages with apt-get",
   "id": "apt_setup",
   "installsAfter": [
      "ghcr.io/devcontainers/features/common-utils"
   ],
   "name": "APT Setup",
   "options": {
      "filepaths": {
         "default": "/usr/local/share/environments/apt.txt",
         "description": "Newline-separaed list of absolute paths to text files containing apt package specifications.",
         "type": "string"
      },
      "log_dir": {
         "default": "/var/log/conda_setup",
         "description": "Absolute path to a directory to write log files.",
         "type": "string"
      }
   },
   "version": "0.1.0"
}
